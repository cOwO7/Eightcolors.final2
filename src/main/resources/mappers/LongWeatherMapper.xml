<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.springbootfinal.app.mapper.LongWeatherMapper">

	<select id="getLongWeatherData" resultType="com.springbootfinal.app.domain.LongWeatherDto">
        SELECT 
            * 
        FROM long_weather 
        WHERE reg_id = #{regId} AND tm_fc = #{tmFc}
    </select>

</mapper>